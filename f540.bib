@article{Rohilla2019,
abstract = {Incandescent lamp has an important role in the electrical lighting application and are used across the world. It has been used as the classic lighting solution for many years and still consider as parent source of lighting. These lamps use tungsten filament to emit light. Being a resistive device, incandescent lamp maintains a high power factor. In this paper, nonlinear nature of resistance of incandescent lamp has been experimentally verified with the help of few laboratory instruments. Based on this study, relationship between various electrical parameters is developed to understand the nonlinearity of incandescent lamp. This study is important for power requirement and efficiency analysis of any lighting infrastructure.},
author = {Rohilla, Yogesh and Kumar, Dinesh},
doi = {10.1109/ICA-SYMP.2019.8646204},
