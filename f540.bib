isbn = {9781538677742},
journal = {2019 1st International Symposium on Instrumentation, Control, Artificial Intelligence, and Robotics, ICA-SYMP 2019},
keywords = {Incandescent lamp,lighting,nonlinear resistance,tungsten filament.},
pages = {73--77},
publisher = {IEEE},
title = {{Nonlinear Nature of Incandescent Lamp: An Experimental Investigation}},
year = {2019}
}
